<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(sponsor) -%>">
  <span class='title'>
    <%= sponsor.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_sponsors_admin_sponsor_path(sponsor),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.sponsors_admin_sponsor_path(sponsor),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => sponsor.name),
        :method => :delete %>
  </span>
</li>
